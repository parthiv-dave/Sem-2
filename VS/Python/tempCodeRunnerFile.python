def utopian_tree(n):
    height = 1  # Initial height
    for cycle in range(n):
        if cycle % 2 == 0:  # Monsoon (even index)
            height *= 2
        else:  # Summer (odd index)
            height += 1
    return height

# Input handling
T = int(input())  # Number of test cases
for _ in range(T):
    N = int(input())  # Number of cycles
    print(utopian_tree(N))
